{
 "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": "2",
        "useBuiltIns": "usage"
      },
      "react"
    ],
    "@babel/preset-react"
 ],
 "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      { "corejs": 2 }
    ],
    "react-hot-loader/babel"
  ]
}